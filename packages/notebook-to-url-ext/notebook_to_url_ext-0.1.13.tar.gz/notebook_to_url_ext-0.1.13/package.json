{
    "name": "notebook_to_url_ext",
    "version": "0.1.13",
    "description": "A JupyterLite extension to compress and decompress notebook text",
    "main": "notebook-to-url-lib/index.js",
    "keywords": [
        "jupyter",
        "jupyterlab",
        "jupyterlab-extension"
      ],
    "scripts": {
      "build": "webpack"
    },
    "jupyterlite": {
        "liteExtension": true
    },
    "jupyterlab": {
        "extension": true,
        "liteExtension":true,
        "outputDir": "notebook-to-url-lib",
        "_build": {
            "liteExtension":true,
            "load": "remoteEntry.js",
            "extension": "index.js"
          }
      },
    "dependencies": {
      "@jupyterlite/server": "^0.1.0",
      "lz-string": "^1.4.4",
      "@jupyterlab/notebook": "<4.4.0",
      "@jupyterlab/apputils": "<4.4.0",
      "@jupyterlab/application": "<4.4.0"
    },
    "devDependencies": {
      "webpack": "^5.0.0",
      "webpack-cli": "^4.0.0",
      "babel-loader": "^8.0.0",
      "@babel/core": "^7.0.0",
      "@babel/preset-env": "^7.0.0",
      "raw-loader": "^4.0.0",
      "file-loader": "^6.0.0",
      "url-loader": "^4.0.0"
    }
  }
  